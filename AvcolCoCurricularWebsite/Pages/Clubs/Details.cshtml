@page "{id:int?}"
@model AvcolCoCurricularWebsite.Pages.Clubs.DetailsModel

@{
    ViewData["Title"] = "Details | Club";
}

<h1>Details</h1>

<div>
    <h4>Club</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Club.Activity)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Club.Activity.ActivityName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Club.Day)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Club.Day)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Club.StartTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Club.StartTime)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Club.EndTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Club.EndTime)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Club.ClubID">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>